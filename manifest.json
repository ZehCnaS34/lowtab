{
	"manifest_version": 2,
	"name": "lowtab",
	"description": "A remake of the chrome extension 'no more tabs'",
	"version": "1.0",
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"permissions": [
		"activeTab",
		"tabs"
	],
	"icons": { 
		"16":  "logo16.png",
		"48":  "logo48.png",
		"128": "logo128.png" 
	}

}
